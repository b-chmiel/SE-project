{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@chakra-ui/icons": "^1.0.3",
        "@chakra-ui/react": "^1.0.4",
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.1.4",
        "@emotion/styled": "^11.0.0",
        "@testing-library/jest-dom": "^5.11.6",
        "@testing-library/react": "^10.4.9",
        "@testing-library/user-event": "^12.6.0",
        "@types/jest": "^25.2.3",
        "@types/node": "^12.19.9",
        "@types/react": "^16.14.2",
        "@types/react-datepicker": "^3.1.2",
        "@types/react-dom": "^16.9.10",
        "@types/react-router-dom": "^5.1.6",
        "axios": "^0.21.1",
        "date-fns": "^2.16.1",
        "formik": "^2.2.6",
        "framer-motion": "^3.1.1",
        "react": "^17.0.1",
        "react-datepicker": "^3.3.0",
        "react-dom": "^17.0.1",
        "react-icons": "^3.11.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "typeface-roboto": "^1.1.13",
        "typescript": "^3.9.7",
        "web-vitals": "^0.2.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "git-format-staged": "^2.1.0",
        "husky": "^4.3.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' ."
        }
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/path/to/dir/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        },
        "coverageReporters": [
            "text"
        ]
    }
}
